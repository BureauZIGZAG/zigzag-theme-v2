    "postcss": "^8.4.18",
    "postcss-nested": "^6.0.0",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.1",
    "terser": "^5.16.0",
    "vite": "^3.1.8",
    "vite-plugin-glob": "^0.3.2",
    "vite-plugin-live-reload": "^3.0.1",
    "vite-plugin-sass-glob-import": "^1.4.0"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "aos": "^2.3.4",
